      <ITEMS> 
        <ITEM NAME="SHEET" FIELD="String"/> 
      </ITEMS> 
    </PARAMETER>  
    <PARAMETER FIELD="CHECK" NAME="EVALUATE_ALL_FORMULAS" NUM_ROW="50"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="CHECK" NAME="ENCRYPT_FILE" NUM_ROW="60"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="TEXT" NAME="PASSWORD" NUM_ROW="60" REQUIRED="true" SHOW_IF="ENCRYPT_FILE == 'true')"> 
      <DEFAULT>"secret"</DEFAULT> 
    </PARAMETER> 
  </PARAMETERS>  
