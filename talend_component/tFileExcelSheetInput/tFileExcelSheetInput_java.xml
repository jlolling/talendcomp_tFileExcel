<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER AUTHOR="cimt AG" COMPATIBILITY="ALL" DATA_AUTO_PROPAGATE="true" HAS_CONDITIONAL_OUTPUTS="false" PLATEFORM="ALL" RELEASE_DATE="20170126" SERIAL="" STARTABLE="true" STATUS="PROD" VERSION="8.3"> 
    <SIGNATURE/> 
  </HEADER>  
  <FAMILIES> 
    <FAMILY>File/Spreadsheet</FAMILY> 
  </FAMILIES>  
  <DOCUMENTATION> 
    <URL>http://jan-lolling.de/talend/components/help/tFileExcelSheetInput.pdf</URL> 
  </DOCUMENTATION>  
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="1" MAX_OUTPUT="1"/>  
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="0"/>  
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>  
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1"/>  
    <CONNECTOR CTYPE="COMPONENT_OK"/>  
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>  
    <CONNECTOR CTYPE="RUN_IF"/> 
  </CONNECTORS>  
  <PARAMETERS> 
    <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="10" REPOSITORY_VALUE="EXCEL"/>  
    <PARAMETER NAME="WORKBOOK" FIELD="COMPONENT_LIST" FILTER="tFileExcelWorkbookOpen" NUM_ROW="2" REQUIRED="true"/>  
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="2"> 
      <DEFAULT/> 
    </PARAMETER>  
    <PARAMETER FIELD="TEXT" NAME="SHEET_NAME" NUM_ROW="10" REQUIRED="true"> 
      <DEFAULT>0</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="TEXT" NAME="ROW_START_INDEX" NUM_ROW="20"> 
      <DEFAULT>1</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="TEXT" NAME="ROW_STOP_INDEX" NUM_ROW="20"> 
      <DEFAULT/> 
    </PARAMETER>  
    <PARAMETER FIELD="CHECK" NAME="STOP_AT_EMPTY_ROW" NUM_ROW="25"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="CHECK" NAME="SKIP_EMPTY_ROW" NUM_ROW="25" SHOW_IF="STOP_AT_EMPTY_ROW == 'false'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="CHECK" NAME="USE_COLUMN_POSITIONING" NUM_ROW="30"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="CHECK" NAME="CONFIG_COLUMN_BY_HEADER" NUM_ROW="30" SHOW_IF="USE_COLUMN_POSITIONING == 'true'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="CHECK" NAME="CONFIG_HEADER_BY_REGEX" NUM_ROW="30" SHOW_IF="(USE_COLUMN_POSITIONING == 'true') and (CONFIG_COLUMN_BY_HEADER == 'true')"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="TEXT" NAME="HEADER_ROW_INDEX" NUM_ROW="50" REQUIRED="true" SHOW_IF="(USE_COLUMN_POSITIONING == 'true') and (CONFIG_COLUMN_BY_HEADER == 'true')"> 
      <DEFAULT>1</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="TEXT" NAME="COLUMN_START_INDEX" NUM_ROW="50" SHOW_IF="USE_COLUMN_POSITIONING == 'false'" REQUIRED="true" REPOSITORY_VALUE="FIRST_COLUMN"> 
      <DEFAULT>"A"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="COLUMN_CONFIG" FIELD="TABLE" NUM_ROW="60" NB_LINES="10" SHOW_IF="USE_COLUMN_POSITIONING == 'true'"> 
      <ITEMS BASED_ON_SCHEMA="true"> 
        <ITEM NAME="POSITION" FIELD="String" SHOW_IF="CONFIG_COLUMN_BY_HEADER =='false'"/>  
        <ITEM NAME="HEADER_NAME" FIELD="String" SHOW_IF="CONFIG_COLUMN_BY_HEADER =='true'"/>  
        <ITEM NAME="IGNORE_MISSING" FIELD="CHECK" SHOW_IF="CONFIG_COLUMN_BY_HEADER =='true'"/>  
        <ITEM NAME="READ_COMMENT" FIELD="CHECK"/>  
        <ITEM NAME="KEEP_LAST" FIELD="CHECK"/>  
        <ITEM NAME="IGNORE_ERROR" FIELD="CHECK"/>  
        <ITEM NAME="READ_CELL_STYLE" FIELD="CHECK"/> 
      </ITEMS> 
    </PARAMETER> 
  </PARAMETERS>  
  <ADVANCED_PARAMETERS> 
    <PARAMETER NAME="NUMBERFORMAT_LOCALE" FIELD="TEXT" NUM_ROW="10"> 
      <DEFAULT>"en"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="USE_GROUPING" FIELD="CHECK" NUM_ROW="10"> 
      <DEFAULT>true</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="OVERRIDE_EXCEL_NUMBER_FORMAT" FIELD="CHECK" NUM_ROW="11"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="DEFAULT_DATE_FORMAT" FIELD="TEXT" NUM_ROW="12"> 
      <DEFAULT>"yyyyMMddHHmmss"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="RETURN_LINK" FIELD="CHECK" NUM_ROW="15"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="ADD_LINK_TO_LABEL" FIELD="CHECK" NUM_ROW="15" SHOW_IF="RETURN_LINK == 'true'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="TRIMSELECT" FIELD="TABLE" NUM_ROW="30" NB_LINES="20"> 
      <ITEMS BASED_ON_SCHEMA="true"> 
        <ITEM NAME="TRIM" FIELD="CHECK"/> 
      </ITEMS> 
    </PARAMETER>  
    <PARAMETER NAME="RELEASE_LABEL_20170126" FIELD="LABEL" COLOR="0;0;0" NUM_ROW="900">
      <DEFAULT>Release: 8.3 build at: 20170126</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>  
  <CODEGENERATION> 
    <IMPORTS/> 
  </CODEGENERATION>  
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/>  
    <RETURN AVAILABILITY="FLOW" NAME="CURRENT_ROW_INDEX" TYPE="id_Integer"/>  
    <RETURN AVAILABILITY="AFTER" NAME="LAST_ROW_INDEX" TYPE="id_Integer"/>  
    <RETURN AVAILABILITY="AFTER" NAME="MAX_ROW_INDEX" TYPE="id_Integer"/>  
    <RETURN AVAILABILITY="BEFORE" NAME="SHEET_NAME" TYPE="id_String"/> 
  </RETURNS>
</COMPONENT>
