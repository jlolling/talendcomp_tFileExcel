	<ADVANCED_PARAMETERS>
		<PARAMETER FIELD="CHECK" NAME="REGISTER_BACKPORT_FUNCTIONS"
			NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FUNCTION_ADDONS" FIELD="TABLE" NUM_ROW="10">
			<ITEMS>
				<ITEM NAME="FUNC_NAME" FIELD="TEXT" />
				<ITEM NAME="FUNC_CLASS" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="USE_DEFAULT_XML_PARSER"
			NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="tFileExcelPOI" MODULE="FileExcelPOI-6.10.jar" REQUIRED="true" />
			<IMPORT NAME="dom4j" MODULE="dom4j-1.6.1.jar" REQUIRED_IF="USE_DEFAULT_XML_PARSER == 'false'" />
