<COMPONENT>
	<HEADER AUTHOR="Jan Lolling" COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="false" PLATEFORM="ALL" RELEASE_DATE="20150610A"
		SERIAL="" STARTABLE="true" STATUS="PROD" VERSION="6.10">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>File/Spreadsheet</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="10" REPOSITORY_VALUE="EXCEL" />

		<PARAMETER FIELD="CHECK" NAME="CREATE_EMPTY_WORKBOOK"
			NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="FILE" NAME="FILENAME" NUM_ROW="20"
			REQUIRED="true" REPOSITORY_VALUE="FILE_PATH" SHOW_IF="CREATE_EMPTY_WORKBOOK == 'false'">
			<DEFAULT>"/path/to/excel_input_file"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SPREADSHEET_TYPE" FIELD="CLOSED_LIST"
			NUM_ROW="20" SHOW_IF="CREATE_EMPTY_WORKBOOK == 'true'">
			<ITEMS DEFAULT="XLS">
				<ITEM NAME="XLS" VALUE="XLS" />
				<ITEM NAME="XLSX" VALUE="XLSX" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="CONFIGTEXT" FIELD="LABEL" COLOR="0;0;0"
			NUM_ROW="24">
			<DEFAULT>The memory saving mode currently applies only to the writing of large files, not to the reading!</DEFAULT>
		</PARAMETER>		
		<PARAMETER FIELD="CHECK" NAME="CREATE_STREAMING_WORKBOOK"
			NUM_ROW="25">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="DIE_IF_FILE_NOT_EXISTS"
			NUM_ROW="30" SHOW_IF="CREATE_EMPTY_WORKBOOK == 'false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="DECRYPT_FILE" NUM_ROW="40"
			SHOW_IF="(CREATE_STREAMING_WORKBOOK == 'false') and (CREATE_EMPTY_WORKBOOK == 'false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="PASSWORD" NUM_ROW="40"
			REQUIRED="true"
			SHOW_IF="(DECRYPT_FILE == 'true') and (CREATE_STREAMING_WORKBOOK == 'false') and (CREATE_EMPTY_WORKBOOK == 'false')">
			<DEFAULT>"secret"</DEFAULT>
		</PARAMETER>
