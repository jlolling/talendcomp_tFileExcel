    <PARAMETER FIELD="TEXT" NAME="BYTEARRAY" NUM_ROW="21" REQUIRED="true" REPOSITORY_VALUE="BYTE_ARRAY" SHOW_IF="(CREATE_EMPTY_WORKBOOK == 'false') and (USE_BYTEARRAY == 'true')"> 
