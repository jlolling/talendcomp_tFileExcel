<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER AUTHOR="cimt AG" COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="true" HAS_CONDITIONAL_OUTPUTS="false" PLATEFORM="ALL"
		RELEASE_DATE="20140708A" SERIAL="" STARTABLE="false" STATUS="PROD"
		VERSION="6.4">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>File/Spreadsheet</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL>
		</URL>
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="EXCEL" />
			
		<PARAMETER NAME="WORKBOOK" FIELD="COMPONENT_LIST"
			FILTER="tFileExcelWorkbookOpen" NUM_ROW="2" REQUIRED="true" />
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="2">
			<DEFAULT />
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="SHEET_NAME" NUM_ROW="10" REQUIRED="true">
			<DEFAULT>"sheet"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="CREATE_SHEET_AS_COPY" NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="SOURCE_SHEET_NAME" NUM_ROW="15" REQUIRED="true" SHOW_IF="CREATE_SHEET_AS_COPY == 'true'">
			<DEFAULT>"template"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="WRITE_IN_COLUMNS" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="APPEND_ROWS" NUM_ROW="30" SHOW_IF="WRITE_IN_COLUMNS == 'false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="ROW_START_INDEX" NUM_ROW="30"	SHOW_IF="APPEND_ROWS=='false'">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="INCLUDE_HEADER" NUM_ROW="40">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="USE_COLUMN_POSITIONING" NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="COLUMN_START_INDEX" NUM_ROW="50"
			SHOW_IF="USE_COLUMN_POSITIONING == 'false'" >
			<DEFAULT>"A"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="COLUMN_CONFIG" FIELD="TABLE" NUM_ROW="60" NB_LINES="10">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="HEADER" FIELD="String" SHOW_IF="INCLUDE_HEADER=='true'"/>
				<ITEM NAME="POSITION" FIELD="String" SHOW_IF="USE_COLUMN_POSITIONING=='true'"/>
				<ITEM NAME="FORMAT" FIELD="String" SHOW_IF="REUSE_STYLE == 'false'" />
				<ITEM NAME="IS_AUTO_SIZE" FIELD="CHECK" />
				<ITEM NAME="IS_COMMENT" FIELD="CHECK" />
				<ITEM NAME="IS_HYPERLINK" FIELD="CHECK" />
				<ITEM NAME="GROUP_ROW" FIELD="CHECK" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="WRITE_NULL" NUM_ROW="65">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="REMOVE_LAST_EMPTY_ROWS" NUM_ROW="65">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="FREEZE_ROW" NUM_ROW="70">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="FREEZE_COL" NUM_ROW="70">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="GROUP_COLS" NUM_ROW="70">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="REUSE_STYLE" NUM_ROW="80" SHOW_IF="WRITE_IN_COLUMNS == 'false'" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="REUSE_STYLE_ALTERNATING" NUM_ROW="80" SHOW_IF="REUSE_STYLE == 'true'" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="REUSE_CF" NUM_ROW="81" SHOW_IF="WRITE_IN_COLUMNS == 'false'" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="REUSE_FIRST_ROW_HEIGHT" NUM_ROW="81" SHOW_IF="WRITE_IN_COLUMNS == 'false'" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="EXTEND_TABLE_CELL_RANGE" NUM_ROW="82" SHOW_IF="WRITE_IN_COLUMNS == 'false'" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER FIELD="CHECK" NAME="DEBUG" NUM_ROW="10" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
	</CODEGENERATION>
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="LAST_ROW_INDEX" TYPE="id_Integer" />
	</RETURNS>
</COMPONENT>
